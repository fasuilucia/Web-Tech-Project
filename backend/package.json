{
  "name": "attendance-monitoring-backend",
  "version": "1.0.0",
  "description": "Backend REST API for attendance monitoring web application",
  "main": "server.js",
  "scripts": {
    "start": "npx sequelize-cli db:migrate && node server.js",
    "dev": "nodemon server.js",
